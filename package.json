    "umi": "2.3.5",
    "umi-plugin-react": "^1.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "jest": {
    "verbose": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
