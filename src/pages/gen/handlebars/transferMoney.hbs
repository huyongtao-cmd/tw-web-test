<style>
  body {
    font-family: verdana, "宋体";
    font-size: 12px;
  }

  @media print {
    .page-break {
      page-break-after: always;
    }
  }

  @media print {
    .no-print {
      display: none;
    }
  }

  .tw-content {
    width: 100%;
    margin: 0 auto
  }

  .tw-title {
    font-size: 20px;
    text-align: center;
    width: 100%;
  }

  .tw-row {
    width: 100%;
    padding: 10px;
    clear: both;
  }

  .tw-table {
    width: 99%;
    margin-bottom: 20px;
    border-collapse: collapse;
    padding: 0;
  }

  .tw-cl {
    clear: both;
  }

  /*表格-表头*/
  .tw-th {
    text-align: center;
    background-color: #d9d9d9;
    min-height: 35px;
  }

  .tw-th > td, .tw-th > th {
    vertical-align: middle;
    border: 1px #000000 solid;
    font-size: 10px;
  }

  /*表格-行*/
  .tw-tr {
    vertical-align: middle;
    height: 35px;
  }

  .tw-tr td, .tw-tr th {
    border: 1px #000000 solid;
    padding-left: 10px;
    font-size: 10px;
  }

  .tw-tr .tw-td-c {
    text-align: center !important;
  }

  /*居左*/
  .tw-tr .tw-td-l {
    text-align: left !important;
  }

  /*居右*/
  .tw-tr .tw-td-r {
    text-align: right !important;
  }

  /*单元格名称*/
  .tw-w-c1 {

  }

  /*单元格值*/
  .tw-w-c2 {

  }
</style>
  
  <div class="tw-content">
    <div class="tw-title  tw-cl">资金划款流程</div>
    <div class="tw-row">
      资金划款编号：{{ formData.transferNo }}
      <table class="tw-table">
        <tr class="tw-tr">
          <td class="tw-w-c1 tw-td-l">流程编号：{{ formData.docNo }}</td>
          <td class="tw-w-c1 tw-td-l">流程名称：{{ formData.docName }}</td>
        </tr>
      </table>

      划款相关信息
      <table class="tw-table">
        <tr class="tw-tr">
          <td class="tw-w-c1 tw-td-l">申请人</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.applicantUserName }}</td>
          <td class="tw-w-c1 tw-td-l">申请日期</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.applicantTime }}</td>
        </tr>
        <tr class="tw-tr">         
          <td class="tw-w-c1 tw-td-l">申请人所属BU</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.applicantBuIdName }}</td>
          <td class="tw-w-c1 tw-td-l">划款公司</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.transferCompanyName }}</td>
        </tr>
        <tr class="tw-tr">          
          <td class="tw-w-c1 tw-td-l">划款账号</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.transferAccount }}</td>
           <td class="tw-w-c1 tw-td-l">收款公司</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.collectionCompanyName }}</td>
        </tr>
        <tr class="tw-tr">        
          <td class="tw-w-c1 tw-td-l">收款账号</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.collectionAccount }}</td>
          <td class="tw-w-c1 tw-td-l">划款金额</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.transferMoney }}</td>
        </tr>
        <tr class="tw-tr">          
          <td class="tw-w-c1 tw-td-l">支付方式</td>
          <td class="tw-w-c1 tw-td-l">{{ formData.payWayName }}</td>
          <td class="tw-w-c1 tw-td-1">状态</td>
          <td class="tw-w-c1 tw-td-1">{{ formData.apprStatusName}}</td>
        </tr>
        <tr class="tw-tr">
          <td class="tw-w-c1 tw-td-l">划款说明</td>
          <td class="tw-w-c1 tw-td-l" colspan="3">{{ formData.transferNote }}</td>
        </tr>
      </table>

      <table class="tw-table">
        <tr>
          <td colspan="6">流转意见</td>
        </tr>
        <tr class="tw-th">
          <td>日期时间</td>
          <td>操作者</td>
          <td>岗位</td>
          <td>节点</td>
          <td>操作类型</td>
          <td>流转意见</td>
        </tr>
        {{#each logList}}
          <tr class="tw-tr">
            <td class="tw-td-c">{{logTime}}</td>
            <td class="tw-td-c">{{workerNames}}</td>
            <td class="tw-td-c">{{workerRoles}}</td>
            <td class="tw-td-c">{{taskName}}</td>
            <td class="tw-td-c">{{resultDesc}}</td>
            <td class="tw-td-l">{{remark}}</td>
          </tr>
        {{/each}}
      </table>

    </div>

  </div>
